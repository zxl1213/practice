#include <iostream>    
#include <string>    
using namespace std;    
void Test(){    
  string s;    
  s.reserve(100);    
  cout<<s.size()<<endl;    
  cout<<s.capacity()<<endl;    
  s.reserve(50);                                                                             
  cout<<s.size()<<endl;    
  cout<<s.capacity()<<endl;    
    
}    
int main(){    
  Test();    
}  
