#include <iostream>                                                                                       
using namespace std;    
class Time{    
  public:    
    Time(){    
      cout<<"Time()"<<endl;    
      _hour=0;    
      _minute=0;    
      _second=0;    
    }    
  private:    
    int _hour;    
    int _minute;    
    int _second;    
};    
class Date{    
  private:    
    int _year;    
    int _month;    
    int _day;    
    //默认构造函数会调用自定值成员的默认成员函数    
    Time _t;    
};    
int main(){    
  Date d;
  return 0;
}    
